                "$ref": "#/components/schemas/Store"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Store"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Store"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "delete": {
        "tags": [
          "Stores"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
